isAble <- function(D, T, B) {
# D = Total Distance to Travel in mm.
# T = Number of jumps that must be taken.
# B = Length of a big jump in mm.
# S = Small Jump, which is always 1mm.
S <- 1
print(c(D,T,B))
}
